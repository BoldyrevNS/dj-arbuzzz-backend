{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": ["maintenance", "dependencies", "chore"]
    },
    {
      "title": "## ğŸ” Security",
      "labels": ["security"]
    },
    {
      "title": "## ğŸ¨ Styling",
      "labels": ["style", "ui", "css"]
    },
    {
      "title": "## âš¡ Performance",
      "labels": ["performance"]
    },
    {
      "title": "## ğŸ§ª Testing",
      "labels": ["test"]
    },
    {
      "title": "## ğŸ”„ Refactoring",
      "labels": ["refactor"]
    },
    {
      "title": "## ğŸŒ CI/CD",
      "labels": ["ci", "cd"]
    }
  ],
  "ignore_labels": [
    "ignore",
    "wontfix",
    "duplicate",
    "invalid"
  ],
  "sort": "ASC",
  "template": "#{{CHANGELOG}}",
  "pr_template": "- #{{TITLE}} (#{{NUMBER}}) by @#{{AUTHOR}}",
  "empty_template": "No changes",
  "label_extractor": [
    {
      "pattern": "^(feature|feat)(\\(.+\\))?:",
      "target": "feature"
    },
    {
      "pattern": "^(fix|bugfix)(\\(.+\\))?:",
      "target": "bug"
    },
    {
      "pattern": "^docs(\\(.+\\))?:",
      "target": "documentation"
    },
    {
      "pattern": "^(style|design)(\\(.+\\))?:",
      "target": "style"
    },
    {
      "pattern": "^(refactor|refactoring)(\\(.+\\))?:",
      "target": "refactor"
    },
    {
      "pattern": "^(perf|performance)(\\(.+\\))?:",
      "target": "performance"
    },
    {
      "pattern": "^test(\\(.+\\))?:",
      "target": "test"
    },
    {
      "pattern": "^(chore|maintenance)(\\(.+\\))?:",
      "target": "maintenance"
    },
    {
      "pattern": "^ci(\\(.+\\))?:",
      "target": "ci"
    },
    {
      "pattern": "^security(\\(.+\\))?:",
      "target": "security"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "base_branches": [
    "main",
    "master"
  ]
}
